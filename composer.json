{
    "name": "matheus-rosa/php-interactor",
    "type": "library",
    "description": "A single purpose object library built for PHP",
    "keywords": [
        "interactor",
        "single-purpose",
        "single-purpose object",
        "solid"
    ],
    "autoload": {
        "psr-4": {
            "MatheusRosa\\PhpInteractor\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "Matheus Rosa",
            "email": "matheus.alves.rosa@outlook.com"
        }
    ],
    "config": {
        "process-timeout": 0,
        "sort-packages": true
    },
    "require": {
        "php": ">=5.4"
    },
    "require-dev": {
        "phpunit/phpunit": "^10",
        "squizlabs/php_codesniffer": "*"
    }
}
